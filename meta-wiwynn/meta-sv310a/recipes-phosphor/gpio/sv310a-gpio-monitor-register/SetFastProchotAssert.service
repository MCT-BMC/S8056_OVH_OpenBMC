[Unit]
Description=Set Fast Prochot Assert

[Service]
Type=oneshot

ExecStart=/usr/bin/busctl call "xyz.openbmc_project.Logging.IPMI" "/xyz/openbmc_project/Logging/IPMI" "xyz.openbmc_project.Logging.IPMI" "IpmiSelAdd" ssaybq "Prochot" "/xyz/openbmc_project/sensors/event/Prochot" 0x03 0x8a 0xff 0xff yes 0x20

ExecStopPost=/usr/bin/busctl set-property xyz.openbmc_project.GpioMonitor /xyz/openbmc_project/gpio/event xyz.openbmc_project.GpioEvent FAST_PROCHOT_N s "Idle"
